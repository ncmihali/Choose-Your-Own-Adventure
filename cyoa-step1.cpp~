#include <string>
#include <vector>

#include "page.cpp"
using namespace std;

int main(int argc, char ** argv) {
  if (argc != 2)
    perror("not enough argument\n");
  Page new_page(argv[1]);
  if (new_page.check_errors(argv[1]) == false)
    cout << "good\n";
  else
    cout << "bad\n";

  new_page.print_page();

  return EXIT_SUCCESS;
}
